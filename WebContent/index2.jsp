        self.show().Float({'floatRight' : picfloat, 'topSide' : ((picfloat == 1) ? right_top : left_top)});
        
        // ie6 png bug
        if (navigator.userAgent.toLowerCase().indexOf('msie 6') != -1) {
        	$.each(self.find('img'), function(){
