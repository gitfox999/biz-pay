        	});
        }
        
        if (picfloat) {
            right_top = right_top + 10 + (self.height() || 150);
        } else {
            left_top = left_top + 10 + (self.height() || 150);
        }
        
        self.hover(function() {
            $(this).find('a > img:first').css('display', 'none');
            $(this).find('a > img:last').css('display', 'block');
